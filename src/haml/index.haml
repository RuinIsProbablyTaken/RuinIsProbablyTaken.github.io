%html
  %head
    %link{:rel => "stylesheet", :type => "text/css", :href => "https://fonts.googleapis.com/css?family=Dosis:300,500|Open+Sans:400,600&subset=latin-ext"}
    /build:css css/style.min.css
    - for i in ["bootstrap", "devicon", "font-awesome"]
      %link{:rel => "stylesheet", :type => "text/css", :href => "/css/#{i}.min.css"}
    %link{:rel => "stylesheet", :type => "text/css", :href => "/css/main.css"}
    /endbuild

    %meta{:name => "viewport", :content => "width=device-width, initial-scale=1.0"}

  %body.text-center#top{:data => {:spy => "scroll", :target => ".navbar", :offset => "50"}}
    .background
    %nav.navbar.navbar-fixed-top{:role => "navigation"}
      .container
        .navbar-header
          %button.navbar-toggle.collapsed{:data => {:toggle => "collapse", :target => "#navigation"}}
            %span.sr-only Toggle navigation

            - for i in [0...3]
              %span.icon-bar

        .collapse.navbar-collapse.navbar-right#navigation
          %ul.nav.navbar-nav.text-capitalize
            - for i in ["about", "quote", "skills", "work", "contact"]
              %li
                %a{:href => "##{i}"}= i
    %header#intro
      .profile-image
      .row
        .col-xs-12.col-md-5.text-left
          .hello
            %h1
              %b Hello,
              %br
              I'm Irina
        .col-xs-12.col-md-2
          %button.btn.btn-round.btn-overlay
            %i.fa.fa-chevron-down
    
