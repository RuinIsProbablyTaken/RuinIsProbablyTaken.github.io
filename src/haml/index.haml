-#
  Moved here due to this code not working(although it should). Leaving it for future reference.
  - skills = (opts) ->
    - for row in opts
      .row
      - for column in row
        .skill.col-xs-12.col-md-6
          .col-xs-12.col-sm-3.text-center
            %i.devicon{:class => "devicon-#{column.icon}-plain"}
          .graph.col-xs-12.col-sm-9
            .text.row
              .col-xs-4
                %h4
                  %i.fa.fa-star.small
                  %span Beginner
              .col-xs-4
                %h4
                  %i.fa.fa-star.medium
                  %span Intermediate
              .col-xs-4
                %h4
                  %i.fa.fa-star.large
                  %span Expert
            .percentage.row
              .col-xs-4
                .b{:class => "#{column.b}"}
              .col-xs-4
                .i{:class => "#{column.i}"}
              .col-xs-4
                .e{:class => "#{column.e}"}
  :coffeescript
  - skilloptions = {        |
    row: {                  |
      column: {             |
        icon: "html5test",  |
        b: "full",          |
        i: "full",          |
        e: "full"           |
      },                    |
      column: {             |
        icon: "css3test",   |
        b: "full",          |
        i: "full",          |
        e: "full"           |
      }                     |
    }                       |
  }                         |
  = skills(skilloptions)


